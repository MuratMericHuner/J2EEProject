<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
      http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
<navigation-rule>
		<from-view-id>Main.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.logged}</from-action>
			<from-outcome>Logged</from-outcome>
			<to-view-id>/Logged.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Main.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.showUsersVisit}</from-action>
			<from-outcome>UsersVisit</from-outcome>
			<to-view-id>/UsersVisit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Main.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.showActivitiesVisit}</from-action>
			<from-outcome>ActivitiesVisit</from-outcome>
			<to-view-id>/ActivitiesVisit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Logged.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.logout}</from-action>
			<from-outcome>Main</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Users.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.newUser}</from-action>
			<from-outcome>Adduser</from-outcome>
			<to-view-id>/Adduser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Users.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.ModifyUser}</from-action>
			<from-outcome>ModifyUser</from-outcome>
			<to-view-id>/ModifyUser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Logged.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.showActivities}</from-action>
			<from-outcome>Activities</from-outcome>
			<to-view-id>/Activities.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>Logged.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{beanManager.showUsers}</from-action>
			<from-outcome>Users</from-outcome>
			<to-view-id>/Users.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>